	<div>
		<input type="text" class="id"> <input type="password"
			class="pw">
		<button id="submit">Login</button>
	</div>

	<div>
		등급은: <span id="result"> </span>입니다.
	</div>

	<script>
		$(function() {
			$("#submit").click(function() {
				var data = {
					id : $('.id').val(),
					pw : $('.pw').val()
				};

				$.ajax({
					type : "POST",
					url : "/member/login",
					data: data,
					dataType: "application/json",
					success : function(data) {
						console.log(data);
					}
				})
			})
		})
	</script>
