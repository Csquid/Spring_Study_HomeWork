<mapper namespace="com.movie.watch.mapper.MemberMapper">
	<select id="memberLogin" resultType="MemberVO">
		SELECT * FROM MEMBER WHERE
		nickname = #{nickname}
		AND M_PASSWORD= #{m_password}
		
	</select>
